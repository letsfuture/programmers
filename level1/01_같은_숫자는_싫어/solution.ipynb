      "source": [
        "def solution(arr):\n",
        "    answer = []\n",
        "    for i in range(len(arr)):\n",
        "        if i == 0:\n",
        "            answer.append(arr[i])\n",
        "        elif arr[i] != arr[i-1]:\n",
        "            answer.append(arr[i])\n",
        "    return answer"
      ]
    },
